{
  "name": "goingnative",
  "version": "2.0.8",
  "description": "A NodeSchool style workshopper for learning how to write native Node.js addons",
  "main": "goingnative.js",
  "bin": "./goingnative.js",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/rvagg/goingnative.git"
  },
  "scripts": {
    "lint": "standard && jshint --exclude=node_modules .",
