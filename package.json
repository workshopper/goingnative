    "postinstall": "node postinstall.js",
    "release": "standard-version"
  },
  "author": "Rod Vagg <rod@vagg.org> (https://github.com/rvagg)",
  "license": "MIT",
  "dependencies": {
    "after": "~0.8.1",
    "bindings": "^1.5.0",
    "chalk": "^4.1.0",
    "colors-tmpl": "~1.0.0",
    "core-util-is": "~1.0.2",
    "cpr": "~1.0.0",
    "getos": "^3.2.1",
    "js-yaml": "^3.14.0",
    "mkdirp": "^1.0.4",
    "nan": "^2.14.0",
    "node-gyp": "^7.0.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.2",
    "varstring": "~0.2.0",
    "workshopper-adventure": "^6.1.0",
    "workshopper-boilerplate": "~1.1.2",
    "workshopper-exercise": "^3.0.1",
    "workshopper-hooray": "~1.1.0",
    "workshopper-more": "~1.0.1"
  },
  "devDependencies": {
    "jshint": "^2.11.1",
    "proxyquire": "^2.1.3",
    "sinon": "^9.0.2",
    "standard": "^14.3.3",
    "standard-version": "^8.0.0",
    "tape": "^5.0.1"
  },
  "standard-version": {
